{
  "name": "homebridge-rently-lock",
  "displayName": "Rently Lock Plugin",
  "type": "module",
  "version": "3.0.0",
  "private": false,
  "description": "This plugin will allow you to connect your lock from Rently using Homebridge.",
  "author": "Jerome Dormoy",
  "license": "Apache-2.0",
  "homepage": "https://github.com/jdormoy972/homebridge-rently-lock/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdormoy972/homebridge-rently-lock"
  },
  "bugs": {
    "url": "https://github.com/jdormoy972/homebridge-rently-lock/issues"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": "^18.20.4 || ^20.18.0 || ^22.10.0",
    "homebridge": "^1.8.0 || ^2.0.0-beta.0"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "eslint . --max-warnings=0",
    "prepublishOnly": "npm run lint && npm run build",
    "watch": "npm run build && sudo hb-service link",
    "restart_": "sudo hb-service unlink && npm run watch && sudo hb-service restart"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.8.6",
    "eslint": "^9.14.0",
    "homebridge": "^2.0.0-beta.0",
    "axios": "^1.6.8",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  },
  "dependencies": {
    "homebridge-lib": "^7.1.0",
    "axios": "^1.6.8"
  }
}
